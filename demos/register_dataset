#!/bin/sh -e

# Get ascii list of frames with e.g., ls -d -1 $PWD/**/*gwf.  Ideally use DiskCache.
# Better yet: find $PWD -type f -name *gwf
ligo_register_data \
    --rse LIGO-CIT-ARCHIVE \
    --scope ER8 \
    --dataset-name H-H1_HOFT_C00-11239 \
    --debug --verbose \
    --file-cache /home/jclark/Projects/ligo-rucio/data/ER8:H-H1_HOFT_C00-11239.txt
